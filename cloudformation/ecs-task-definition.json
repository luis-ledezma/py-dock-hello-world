{
    "containerDefinitions": [ 
       { 
          "essential": true,
          "image": "DOCKER_IMAGE",
          "logConfiguration": { 
             "logDriver": "awslogs",
             "options": { 
                "awslogs-group" : "/ecs/py-docker-hello-world-container",
                "awslogs-region": "us-east-1",
                "awslogs-stream-prefix": "ecs"
             }
          },
          "name": "py-docker-hello-world-container",
          "portMappings": [ 
             { 
                "containerPort": 8080,
                "hostPort": 8080,
                "protocol": "tcp"
             }
          ]
       }
    ],
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::025066248951:role/ecsTaskExecutionRole",
    "family": "py-docker-hello-world-task",
    "memory": "512",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [ 
        "FARGATE" 
     ]
 }